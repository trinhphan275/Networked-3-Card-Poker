    <children>
        <Label  contentDisplay="CENTER" layoutX="20.0" layoutY="70.0" styleClass="normalText" text="Are you sure you want to quit?" />
        <Button layoutX="40.0" layoutY="130.0" mnemonicParsing="false" styleClass="btns" onAction = "#endGame" text="Quit" />
        <Button layoutX="190.0" layoutY="130.0" mnemonicParsing="false" styleClass="btns" onAction = "#continueGame" text="Continue" />
    </children>
</AnchorPane>
